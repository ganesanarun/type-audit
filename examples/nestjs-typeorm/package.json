{
  "name": "nestjs-typeorm-audit-example",
  "version": "1.0.0",
  "description": "NestJS + TypeORM + PostgreSQL example using @snow-tzu/audit",
  "private": true,
  "scripts": {
    "start": "yarn build && nest start",
    "start:dev": "yarn build && nest start --watch",
    "start:debug": "yarn build && nest start --debug --watch",
    "build": "rm -f tsconfig.tsbuildinfo && tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "clean": "rm -rf dist && rm -rf tsconfig.tsbuildinfo",
    "prebuild": "yarn clean",
    "migration:generate": "npm run typeorm -- migration:generate -d src/config/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/config/data-source.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@snow-tzu/audit": "^0.0.3",
    "nest-winston": "^1.9.4",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
